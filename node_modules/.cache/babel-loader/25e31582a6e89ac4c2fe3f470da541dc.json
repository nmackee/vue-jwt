label for=\"txtEmail\" class=\"form-label\">Email</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"txtEmail\"\n          aria-describedby=\"emailHelp\"\n          v-model=\"email\"\n        />\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"txtPassword\" class=\"form-label\">Password</label>\n        <input\n          type=\"password\"\n          class=\"form-control\"\n          id=\"txtPassword\"\n          v-model=\"password\"\n        />\n      </div>\n      <button type=\"button\" class=\"btn btn-primary\" @click=\"login()\">\n        Submit\n      </button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n    };\n  },\n  computed: {\n    ...mapGetters('auth', {\n      user\n    })\n  },\n  methods: {\n    ...mapActions('auth', {\n      userLogin: 'userLogin'\n    }),\n    async login() {\n      const payload = {\n        email: this.email,\n        password: this.password\n      }\n      await this.userLogin(payload);\n    },\n  },\n};\n</script>"],"sourceRoot":""}]}